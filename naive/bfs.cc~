#include <stdio.h>
#include <deque>

void bfs() 
{
	 printf("in bfs\n");
	 std::deque<unsigned int> front;

	 front.push_back(3);
	 front.push_back(14);
	 front.push_back(22);
	 printf("%u\n", front.front());
	 front.pop_front();
	 printf("%u\n", front.front());

	 return;
	 
}
